    {
        "view": "team",
        "module": "HeatmapScatterPlot",
        "name": "Shot Plot",
        "position": "main",
        "options": {
            "coordinatePath": {
                "x" : 5,
                "y" : 6
            },
            "aggregatedActionsPath": "aggregatedActions",
            "actionGroups": [
                {
                    "name": "Shots",
                    "actions": ["level2", "level1", "miss"]
                }
            ],
            "actionLabels": {
                "level2": "Level 2",
                "level1": "Level 1",
                "level0": "Level 0",
                "miss": "Miss"
            },
            "imgPath": "/img/field.svg"
        }
    },
	{
        "view": "team",
        "module": "HeatmapScatterPlot",
        "name": "Pickup Plot",
        "position": "main",
